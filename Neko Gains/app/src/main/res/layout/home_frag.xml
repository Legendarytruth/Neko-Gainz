<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <FrameLayout
        android:id="@+id/catLayout"
        android:layout_width="match_parent"
        android:layout_height="518dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="-1dp"
        android:layout_marginLeft="0dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="133dp"
        android:clipChildren="false">

        <pl.droidsonroids.gif.GifImageView
            android:id="@+id/CatBody"
            android:layout_width="337dp"
            android:layout_height="428dp"
            android:layout_gravity="center" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/ClothingButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|right"
            android:clickable="true"
            app:backgroundTint="#000000"
            app:srcCompat="@android:drawable/ic_menu_more"
            app:useCompatPadding="true" />

        <TextView
            android:id="@+id/CatFood"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="81dp"
            android:layout_marginLeft="81dp"
            android:layout_marginTop="196dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/Blueberry"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="81dp"
            android:layout_marginLeft="81dp"
            android:layout_marginTop="20dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/Fish"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="81dp"
            android:layout_marginLeft="81dp"
            android:layout_marginTop="253dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/Milk"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="81dp"
            android:layout_marginLeft="81dp"
            android:layout_marginTop="306dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/Money"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_gravity="right|top"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="24dp"
            android:layout_marginRight="50dp"
            android:text="TextView" />

        <ImageButton
            android:id="@+id/CatFoodButton"
            android:layout_width="45dp"
            android:layout_height="40dp"
            android:layout_marginStart="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="185dp"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/catfood_smallicon" />

        <ImageButton
            android:id="@+id/BlueberryButton"
            android:layout_width="45dp"
            android:layout_height="40dp"
            android:layout_gravity="top|left"
            android:layout_marginStart="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="40dp"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/blueberries_smallicon" />

        <ProgressBar
            android:id="@+id/Hunger"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:layout_gravity="right|top"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="15dp"
            android:background="#00FF0000"
            android:max="100"
            android:visibility="visible"
            tools:visibility="visible" />

        <ImageButton
            android:id="@+id/MilkButton"
            android:layout_width="45dp"
            android:layout_height="40dp"
            android:layout_marginStart="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="295dp"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/milk_smallicon" />

        <ImageButton
            android:id="@+id/FishButton"
            android:layout_width="45dp"
            android:layout_height="40dp"
            android:layout_marginStart="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="242dp"
            android:scaleType="fitXY"
            android:tint="#00FFFFFF"
            android:visibility="visible"
            app:srcCompat="@drawable/fish_smallicon"
            tools:srcCompat="@drawable/fish_smallicon" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:layout_gravity="right|top"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="90dp"
            android:layout_marginRight="100dp"
            android:src="@drawable/moneyiconfinal"
            app:srcCompat="@drawable/moneyiconfinal" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:layout_gravity="right|top"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="89dp"
            android:adjustViewBounds="false"
            android:baselineAlignBottom="false"
            android:src="@drawable/hungericonfinal"
            app:srcCompat="@drawable/hungericonfinal" />

        <FrameLayout
            android:id="@+id/PopupRestriction"
            android:layout_width="225dp"
            android:layout_height="280dp"
            android:layout_gravity="bottom|right"
            android:layout_marginRight="40dp"
            android:layout_marginBottom="45dp">

        </FrameLayout>

    </FrameLayout>

    <View
        android:id="@+id/divider3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="-2dp"
        android:layout_marginBottom="79dp"
        android:background="#000000" />

    <View
        android:id="@+id/divider2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="26dp"
        android:background="#000000" />

    <TextView
        android:id="@+id/PetName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:text="PetName"
        android:textSize="40sp" />

    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="129dp"
        android:background="#000000" />

    <ProgressBar
        android:id="@+id/Experience"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="302dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="97dp"
        android:background="#00FF0000"
        android:max="1000"
        android:visibility="visible"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/Level"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="96dp"
        android:text="TextView" />

    <View
        android:id="@+id/divider6"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="79dp"
        android:layout_marginEnd="-1dp"
        android:background="?android:attr/listDivider" />

</RelativeLayout>